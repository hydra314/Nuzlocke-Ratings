<div class="container-fluid">
  <div class="row">
    <div class="col-md-8 info-page-col">
      <h4><%= playerInfo.username %></h4><br>
      <p><b>Rating:</b> <%= playerInfo.rating %></p>

      <p><b>Title:</b>
        <% if(playerInfo.name){ %>
          <%= playerInfo.name %> 
        <% } else { %> 
          None 
        <% } %>
      </p>

      <p><b>Discord:</b>
        <% if(playerInfo.discord){ %>
          <%= playerInfo.discord %> 
        <% } else { %> 
          None 
        <% } %>
      </p>

      <p><b>Personal Page:</b>
        <% if(playerInfo.link){ %>
          <a href="<%= playerInfo.link %>" target="_blank">Link</a> 
        <% } else { %> 
          None 
        <% } %>
      </p>
    </div>
  </div>
</div>