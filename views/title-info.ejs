<div class="table-wrapper">
  <table
    class="table table-hover table-dark table-striped"
    id="title-info-table"
  >
    <thead class="thead-info">
      <tr>
        <th
          scope="col"
          onclick="sortTable(0, 'title-info-table')"
          class="sortable"
        >
          Title
        </th>
        <th
          scope="col"
          onclick="sortTable(1, 'title-info-table')"
          class="sortable"
        >
          Abbreviation
        </th>
        <th
          scope="col"
          onclick="sortTable(2, 'title-info-table')"
          class="sortable"
        >
          Minimum Rating
        </th>
        <th
          scope="col"
          onclick="sortTable(3, 'title-info-table')"
          class="sortable"
        >
          Bronze Challenges Required
        </th>
        <th
          scope="col"
          onclick="sortTable(4, 'title-info-table')"
          class="sortable"
        >
          Silver Challenges Required
        </th>
        <th
          scope="col"
          onclick="sortTable(5, 'title-info-table')"
          class="sortable"
        >
          Gold Challenges Required
        </th>
      </tr>
    </thead>
    <tbody>
      <% for(let title in titles){ %>
      <tr>
        <td><%= titles[title].name %></td>
        <td><%= titles[title].abbreviation %></td>
        <td><%= titles[title].rating_floor %></td>
        <td><%= titles[title].min_bronze_challenges %></td>
        <td><%= titles[title].min_silver_challenges %></td>
        <td><%= titles[title].min_gold_challenges %></td>
      </tr>
      <% } %>
    </tbody>
  </table>
</div>
